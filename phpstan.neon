includes:
    - vendor/larastan/larastan/extension.neon
    - phpstan-baseline.neon

parameters:

    paths:
        - app/

    # Level 5 is a good starting point. Increase gradually to 9 for stricter analysis.
    level: 5

    excludePaths:
        - ./*/*/FileToBeExcluded.php
        - app/Models/Role.php
        - app/Models/UserWebsiteSetting.php

